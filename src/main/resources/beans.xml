<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="actor1" class="com.stackroute.Actor">
        <property name="name" value="Prabhas"></property>
        <property name="gender" value="Male"></property>
        <property name="age" value="32"></property>
    </bean>
    <bean id="actor2" class="com.stackroute.Actor">
        <property name="name" value="Shobha"></property>
        <property name="gender" value="Female"></property>
        <property name="age" value="25"></property>
    </bean>
    <bean id="movie" class="com.stackroute.Movie">
        <property name="actor" ref="actor2"></property>
    </bean>
</beans>
